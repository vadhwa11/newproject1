	<queryString><![CDATA[SELECT  VT.VISIT_ID AS VISIT_ID,
        (CASE HD.PROCEDURE_TYPE WHEN 'M' THEN 'MINOR SURGERY'
                               WHEN 'P' THEN 'PHYSIOTHEROPY' 
                               ELSE 'NURSING CARE' END) NURSING_TYPE,
        NURSING_NAME AS NAME, 
        HD.REQUISITION_DATE AS START_DATE,
        MF.FREQUENCY_NAME AS FREQUENCY,
        DT.NO_OF_DAYS , 
        DT.REMARKS
FROM    PROCEDURE_DT DT
LEFT OUTER JOIN PROCEDURE_HD HD ON DT.PROCEDURE_HD_ID=HD.PROCEDURE_HD_ID
LEFT OUTER JOIN VISIT VT ON VT.VISIT_ID=HD.VISIT_ID
LEFT OUTER JOIN MAS_NURSING_CARE MNC ON DT.PROCEDURE_ID=MNC.NURSING_ID
LEFT OUTER JOIN MAS_FREQUENCY MF ON MF.FREQUENCY_ID=DT.FREQUENCY_ID
WHERE    HD.PATIENT_ID=$P{P_PATIENTID} AND UPPER(VT.VISIT_STATUS)='C' and HD.PROCEDURE_TYPE='P']]></queryString>

