	<queryString><![CDATA[SELECT PATIENT.PATIENT_NAME , PATIENT.SERVICE_NO , MAS_RELATION.RELATION_NAME ,   to_char(VISIT.VISIT_DATE,'DD/MM/YYYY') as VISIT_DATE,
to_char(VISIT.VISIT_DATE,'HH:MI:SS') as VISIT_TIME,
VISIT.TOKEN_NO , MAS_rank.RANK_name , MAS_APPOINTMENT_TYPE.APPOINTMENT_TYPE_NAME , PATIENT.UHID_NO,
FLOOR(MONTHS_BETWEEN(TRUNC(SYSDATE),PATIENT.DATE_OF_BIRTH)/12)||' / '||MAS.ADMINISTRATIVE_SEX_NAME AS AGEGENDER,
MD.DEPARTMENT_NAME
FROM VISIT
LEFT OUTER JOIN PATIENT ON PATIENT.PATIENT_ID = VISIT.PATIENT_ID
LEFT OUTER JOIN MAS_ADMINISTRATIVE_SEX MAS ON MAS.ADMINISTRATIVE_SEX_ID=PATIENT.ADMINISTRATIVE_SEX_ID
LEFT OUTER JOIN MAS_RELATION on MAS_RELATION.RELATION_ID=PATIENT.RELATION_ID
LEFT OUTER JOIN MAS_rank on MAS_rank.rank_ID=PATIENT.rank_ID
LEFT OUTER JOIN PATIENT ON PATIENT.PATIENT_ID = VISIT.PATIENT_ID
LEFT OUTER JOIN mas_department md ON md.department_id = VISIT.department_id
LEFT OUTER JOIN MAS_APPOINTMENT_TYPE on MAS_APPOINTMENT_TYPE.APPOINTMENT_TYPE_ID=VISIT.APPOINTMENT_TYPE_ID
WHERE VISIT.VISIT_ID = $P{VISIT_ID}]]></queryString>

