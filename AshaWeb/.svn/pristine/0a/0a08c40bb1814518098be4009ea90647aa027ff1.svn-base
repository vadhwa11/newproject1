}	


function ResetForm()
{
	$j('#empanelledHospitalCode').val('');
	$j('#empanelledHospitalName').val('');
	$j('#empanelledHospitalAddress').val('');
	$j('#searchEmpanelledHospital').val('');
	
}

function showAll()
{
