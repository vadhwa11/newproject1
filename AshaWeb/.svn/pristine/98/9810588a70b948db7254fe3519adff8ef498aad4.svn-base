*{margin:0px;padding:0px;}

.inner_md_htext{font-size:16px;}

 
#div1{width: 68%;
    height: 28px;
    position: absolute;
    text-align: center;
    display: none;
    line-height: 28px;
    top: 0px;
   left: 3%;
    color: #f5811a;
    font-weight: 700;
				}
	
	
	
/* File Upload */
.fake-shadow {
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.2);
}
.fileUpload {
    position: relative;
    overflow: hidden;
}
.fileUpload #logo-id {
    position: absolute;
    top: 0;
    right: 0;
    margin: 0;
    padding: 0;
    font-size: 33px;
    cursor: pointer;
    opacity: 0;
    filter: alpha(opacity=0);
}
.img-preview {
    max-width: 100%;
}
		
#resultnavigation input.singlePrev {
margin: 0px 0px 0px 10px;
padding: 0px;
background: url(../images/arrowPrev.gif) no-repeat;
height: 15px;
width: 31px;
vertical-align: middle;
border: none;
cursor: pointer;
color: #FFF;
text-align: right;
text-shadow: none;
}

