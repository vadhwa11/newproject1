		return;
	}
	var data = {"hospitalId": <%= hospitalId %>,"employeeId": <%=userId%>,"opdPre":"opdPre", "serviceNo":service_no,"patientName":patient_name,"pageNo":"1"};
	var bClickable = true;
	var url = "getPreConsPatientWatingWeb";
	GetJsonData('tblListofOpdP',data,url,bClickable);
} 

