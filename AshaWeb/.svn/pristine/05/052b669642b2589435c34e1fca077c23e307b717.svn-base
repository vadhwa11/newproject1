			 'medicalCategoryCode':jQuery('#medicalCategoryCode').val(),
			 'medicalCategoryName':jQuery('#medicalCategoryName').val()			 
			
	 } 
	
		    var url = "updateMedicalCategoryDetails";
	        SendJsonData(url,params);
	
