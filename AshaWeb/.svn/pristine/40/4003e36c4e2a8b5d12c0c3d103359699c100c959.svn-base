	<parameter name="VISIT_ID" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT  (CASE  REFERRAL_PATIENT_HD.TREATMENT_TYPE WHEN 'E' THEN 'EXTERNAL' ELSE 'INTERNAL' END) as TREATMENT_TYPE, 
REFERRAL_PATIENT_HD.REFERRAL_INI_DATE , MAS_ICD.ICD_NAME ,(CASE WHEN REFERRAL_PATIENT_HD.TREATMENT_TYPE ='E' THEN  EXT_DEPARTMENT ELSE MAS_DEPARTMENT.DEPARTMENT_NAME END) Speciality,
(CASE WHEN REFERRAL_PATIENT_HD.TREATMENT_TYPE ='E' THEN  EMPANELLED_HOSPITAL_NAME ELSE HOSPITAL_NAME END) HOSPITAL_NAME,INSTRUCTION
FROM REFERRAL_PATIENT_DT
LEFT OUTER JOIN REFERRAL_PATIENT_HD on REFERRAL_PATIENT_HD.REFREAL_HD_ID=REFERRAL_PATIENT_DT.REFREAL_HD_ID
LEFT OUTER JOIN OPD_PATIENT_DETAILS OPD on REFERRAL_PATIENT_HD.OPD_PATIENT_DETAILS_ID=OPD.OPD_PATIENT_DETAILS_ID
LEFT OUTER JOIN MAS_ICD on MAS_ICD.ICD_ID=REFERRAL_PATIENT_DT.DIAGNOSIS_ID 
LEFT OUTER JOIN MAS_DEPARTMENT on MAS_DEPARTMENT.DEPARTMENT_ID=REFERRAL_PATIENT_DT.INT_DEPARTMENT_ID 
LEFT OUTER JOIN MAS_HOSPITAL on MAS_HOSPITAL.HOSPITAL_ID=REFERRAL_PATIENT_HD.INT_HOSPITAL_ID 
LEFT OUTER JOIN MAS_EMPANELLED_HOSPITAL on MAS_EMPANELLED_HOSPITAL.EMPANELLED_HOSPITAL_ID=REFERRAL_PATIENT_HD.EXT_HOSPITAL_ID
WHERE OPD.VISIT_ID=$P{VISIT_ID}]]></queryString>

