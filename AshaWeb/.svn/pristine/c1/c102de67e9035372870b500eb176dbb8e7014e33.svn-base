	<queryString><![CDATA[SELECT  VT.VISIT_ID AS VISIT_ID,
        MMC.MAIN_CHARGECODE_NAME AS INVESTIGATION_TYPE,
        DT.ORDER_DATE  AS INVESTIGATION_DATE,
        DMI.INVESTIGATION_NAME AS INVESTIGATION_NAME 
FROM    DG_ORDER_DT DT 
LEFT OUTER JOIN DG_ORDER_HD HD  ON HD.ORDERHD_ID=DT.ORDERHD_ID 
LEFT OUTER JOIN VISIT VT  ON VT.VISIT_ID=HD.VISIT_ID 
LEFT OUTER JOIN DG_MAS_INVESTIGATION DMI ON DMI.INVESTIGATION_ID=DT.INVESTIGATION_ID 
LEFT OUTER JOIN MAS_MAIN_CHARGECODE MMC ON MMC.MAIN_CHARGECODE_ID=DMI.MAIN_CHARGECODE_ID 
WHERE   HD.PATIENT_ID=$P{P_PATIENTID} AND UPPER(VT.VISIT_STATUS)='C' and HD.VISIT_ID =$P{P_VISITID}]]></queryString>

