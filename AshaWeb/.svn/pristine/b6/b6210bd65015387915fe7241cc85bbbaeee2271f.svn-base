	<queryString><![CDATA[SELECT PATIENT.SERVICE_NO ,PATIENT.EMPLOYEE_NAME ,MAS_RANK.RANK_NAME , MAS_ADMINISTRATIVE_SEX.ADMINISTRATIVE_SEX_NAME, PATIENT.PATIENT_NAME as PATIENT_NAME, 
floor(months_between(trunc(sysdate),PATIENT.DATE_OF_BIRTH)/12) age ,MAS_RELATION.RELATION_NAME as RELATION_NAME,U.user_name AS doctor_name  
FROM PATIENT 
LEFT OUTER JOIN VISIT on PATIENT.PATIENT_ID=VISIT.PATIENT_ID
LEFT OUTER JOIN MAS_RELATION on MAS_RELATION.RELATION_ID=PATIENT.RELATION_ID
LEFT OUTER JOIN MAS_RANK on MAS_RANK.RANK_ID=PATIENT.RANK_ID
LEFT OUTER JOIN USERS U on U.USER_ID=VISIT.INT_DOCTOR_ID
LEFT OUTER JOIN MAS_ADMINISTRATIVE_SEX on MAS_ADMINISTRATIVE_SEX.ADMINISTRATIVE_SEX_ID=PATIENT.ADMINISTRATIVE_SEX_ID
WHERE VISIT.VISIT_ID=$P{VISIT_ID}]]></queryString>

